@model MCAWebAndAPI.Model.ViewModel.Control.ComboBoxVM

@(
 Html.Kendo().DropDownListFor(m => m.Value)
        .BindTo(Model.Choices)
        .Filter("contains")
        .Value(Model.DefaultValue)
        .HtmlAttributes(new {
            @class = "form-control",
            style = "width:100%;"
        })
)
