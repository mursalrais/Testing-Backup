
@{
    ViewBag.Title = "All WBS";
}
<div class="row">
    <div class="col-md-8"></div>
    <div class="col-md-4 pull-right">
        <button id="update-wbs-mapping">Update WBS Mapping</button>
    </div>
</div>

<div class="row-">
    <div class="col-md-12">
        <div id="wbs-grid"></div>
    </div>
</div>

<script type="text/javascript">

    $(document).ready(function () {
        EPMO.Grids.displayWBSGrid("wbs-grid");
        $("#update-wbs-mapping").click(function (e) {
            EPMO.Commands.updateWBSMapping();
        })
    });

</script>