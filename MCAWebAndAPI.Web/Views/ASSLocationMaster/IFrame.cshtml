@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>iframe</title>
</head>
<body>
    <div>
        <iframe width="100%" height="1050" src="http://localhost:15923/ASSLocationMaster/Create" frameborder="0"></iframe>
    </div>
    <script>
        var eventMethod = window.addEventListener ? "addEventListener" : "attachEvent";
        var eventer = window[eventMethod];
        var messageEvent = eventMethod == "attachEvent" ? "onmessage" : "message";

        // Listen to message from child window
        eventer(messageEvent, function (e) {
            if (e.data == "Success") {

                setTimeout(function () {
                    location.href = "https://eceos2.sharepoint.com/sites/mca-dev/dev/Lists/LocationMaster/AllItems.aspx";
                }, 1000);
            }
            console.log('parent received message!:  ', e.data);
        }, false);
    </script>
</body>
</html>
