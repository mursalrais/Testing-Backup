@using MCAWebAndAPI.Model.ViewModel.Form.HR
@model ApplicationDataVM

<div class="col-md-6">
    <div class="form-group">
        @Html.Bootstrap().LabelFor(m => m.Documents).Class("col-md-4 control-label")
        <div class="col-md-8">
            @for (int i = 0; i < Model.Documents.ToArray().Length; i++)
            {
                <span style="display:block">@Model.Documents.ToArray()[i].FileName</span>
            }
        </div>
    </div>
</div>
